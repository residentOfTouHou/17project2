<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cskaoyan.mall.mapper.SystemPermissionMapper">

    <resultMap id="permissionmap" type="com.cskaoyan.mall.bean.generator.SystemPermission">
        <result column="id" property="id"/>
        <result column="label" property="label"/>
        <result column="api" property="api"/>
    </resultMap>

    <select id="selectPermissionByParentId" resultMap="permissionmap">
        select id , label , api from cskaoyan_mall_system_permission where parent_id = #{parentId}
    </select>
</mapper>
