<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="testTables" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/sb_project"
                        userId="root"
                        password="123456">
            <!--是否去除同名表-->
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>
        <!--&lt;!&ndash;
            for oracle
           &ndash;&gt;
        <jdbcConnection driverClass="oracle.jdbc.OracleDriver"
            connectionURL="jdbc:oracle:thin:@127.0.0.1:1521:yycg"
            userId="yycg"
            password="yycg">
        </jdbcConnection>-->

        <!-- 默认false，
            为false把JDBC DECIMAL 和 NUMERIC 类型解析为Integer，
            为 true把JDBC DECIMAL 和 NUMERIC 类型解析为java.math.BigDecimal -->
        <!--<javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>-->

        <!-- javaModelGenerator javaBean生成的配置信息
             targetProject:生成PO类的位置
             targetPackage：生成PO类的类名-->
        <javaModelGenerator targetPackage="com.cskaoyan.mall.bean"
                            targetProject=".\src\main\java">
            <!-- enableSubPackages:是否允许子包,是否让schema作为包的后缀
                 即targetPackage.schemaName.tableName -->
            <property name="enableSubPackages" value="true"/>
            <!-- 从数据库返回的值是否清理前后的空格 -->
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- sqlMapGenerator Mapper映射文件的配置信息
            targetProject:mapper映射文件生成的位置
            targetPackage:生成mapper映射文件放在哪个包下-->
        <sqlMapGenerator targetPackage="com.cskaoyan.mall.mapper"
                         targetProject=".\src\main\resources">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!--
           javaClientGenerator 生成 Model对象(JavaBean)和 mapper XML配置文件 对应的Dao代码
           targetProject:mapper接口生成的位置
           targetPackage:生成mapper接口放在哪个包下

           ANNOTATEDMAPPER
           XMLMAPPER
           MIXEDMAPPER
        -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.cskaoyan.mall.mapper"
                             targetProject=".\src\main\java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator><!---->
        <!-- 指定数据库表 -->

        <!-- 指定所有数据库表 -->

        <!--<table tableName="%"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               enableInsert="false"
               enableDeleteByPrimaryKey="true"
               enableSelectByPrimaryKey="true"
               selectByExampleQueryId="false" ></table>-->

        <!-- 指定数据库表，要生成哪些表，就写哪些表，要和数据库中对应，不能写错！ -->
        <table tableName="cskaoyan_mall_ad" domainObjectName="Ad"/>
        <table tableName="cskaoyan_mall_address" domainObjectName="Address"/>
        <table tableName="cskaoyan_mall_admin" domainObjectName="Admin"/>
        <table tableName="cskaoyan_mall_brand" domainObjectName="Brand"/>
        <table tableName="cskaoyan_mall_cart" domainObjectName="Cart"/>
        <table tableName="cskaoyan_mall_category" domainObjectName="Category"/>
        <table tableName="cskaoyan_mall_collect" domainObjectName="Collect"/>
        <table tableName="cskaoyan_mall_comment" domainObjectName="Comment"/>
        <table tableName="cskaoyan_mall_coupon" domainObjectName="Coupon"/>
        <table tableName="cskaoyan_mall_coupon_user" domainObjectName="CouponUser"/>
        <table tableName="cskaoyan_mall_feedback" domainObjectName="Feedback"/>
        <table tableName="cskaoyan_mall_footprint" domainObjectName="Footprint"/>
        <table tableName="cskaoyan_mall_goods" domainObjectName="Goods"/>
        <table tableName="cskaoyan_mall_goods_attribute" domainObjectName="GoodsAttribute"/>
        <table tableName="cskaoyan_mall_goods_product" domainObjectName="GoodsProduct"/>
        <table tableName="cskaoyan_mall_goods_specification" domainObjectName="GoodsSpecification"/>
        <table tableName="cskaoyan_mall_groupon" domainObjectName="Groupon"/>
        <table tableName="cskaoyan_mall_groupon_rules" domainObjectName="GrouponRules"/>
        <table tableName="cskaoyan_mall_issue" domainObjectName="Issue"/>
        <table tableName="cskaoyan_mall_keyword" domainObjectName="Keyword"/>
        <table tableName="cskaoyan_mall_log" domainObjectName="Log"/>
        <table tableName="cskaoyan_mall_order" domainObjectName="Order"/>
        <table tableName="cskaoyan_mall_order_goods" domainObjectName="OrderGoods"/>
        <table tableName="cskaoyan_mall_permission" domainObjectName="Permission"/>
        <table tableName="cskaoyan_mall_region" domainObjectName="Region"/>
        <table tableName="cskaoyan_mall_role" domainObjectName="Role"/>
        <table tableName="cskaoyan_mall_search_history" domainObjectName="SearchHistory"/>
        <table tableName="cskaoyan_mall_storage" domainObjectName="Storage"/>
        <table tableName="cskaoyan_mall_system" domainObjectName="System"/>
        <table tableName="cskaoyan_mall_topic" domainObjectName="Topic"/>
        <table tableName="cskaoyan_mall_user" domainObjectName="User"/>
        <table tableName="cskaoyan_mall_user_formid" domainObjectName="UserFormid"/>


        <!--      <table schema="" tableName="orders"></table>
             <table schema="" tableName="items"></table>
             <table schema="" tableName="orderdetail"></table>
      -->
